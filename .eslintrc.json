{
    "env": {
        "browser": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
    "extends": [
        "eslint:recommended"
    ],
    "rules": {
        "no-var": "error",
        "semi": "error",
        "max-params": [
            "error",
            4
        ],
        "eqeqeq": "error"
    }
}